{
  "name": "nuxt-coolify",
  "version": "0.1.0",
  "description": "Integrate Nuxt With Coolify to simplify your deployments.",
  "homepage": "https://github.com/justserdar/nuxt-coolify#readme",
  "author": "Serdar Yerdelen",
  "repository": "@justserdar/nuxt-coolify",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint playground",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "docs": "nuxt dev docs",
    "docs:build": "nuxt build docs",
    "docs:prepare": "nuxt prepare docs",
    "docs:preview": "nuxt preview docs",
    "docs:start": "node .output/server/index.mjs"
  },
  "dependencies": {
    "@nuxt/kit": "^3.16.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/eslint-config": "^0.5.7",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.0",
    "@nuxt/test-utils": "^3.17.2",
    "changelogen": "^0.5.7",
    "eslint": "^9.22.0",
    "nuxt": "^3.16.0",
    "vite": "^5.4.14",
    "vitepress": "^1.6.3",
    "vitest": "^2.1.9",
    "vue-tsc": "2.1.6"
  },
  "resolutions": {
    "nuxt-coolify": "workspace:*",
    "typescript": "5.6.3",
    "vue-tsc": "2.1.6"
  },
  "packageManager": "pnpm@10.5.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi",
      "workerd"
    ]
  }
}
